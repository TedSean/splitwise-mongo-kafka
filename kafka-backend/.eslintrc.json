{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-param-reassign":"off",
        "consistent-return": "off",
        "no-underscore-dangle":"off",
        "array-callback-return":"off"
    }
}
